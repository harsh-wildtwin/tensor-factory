---
import BaseLayout from "../layouts/BaseLayout.astro";
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
---
<BaseLayout title="Supplier/Vendor – Tensor Factory" description="Procurement and vendor contact information.">
  <Fragment slot="header"><Header /></Fragment>
  <section class="section">
    <div class="container stack">
      <h1>Supplier / Vendor Information</h1>
      <p>For procurement and vendor inquiries, please reach out via the Contact page, selecting “Supplier/Vendor”. We provide security and compliance details upon request.</p>
    </div>
  </section>
  <Fragment slot="footer"><Footer /></Fragment>
</BaseLayout>


